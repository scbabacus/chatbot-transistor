{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "no-console": false,
    "interface-name": false,
    "object-shorthand-properties-first": false,
    "ter-arrow-parens": false,
    "object-literal-sort-keys": false,
    "variable-name": [true, "allow-leading-underscore"]
  }
}
